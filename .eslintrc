{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "object-curly-spacing": ["error", "always"],
        "no-unused-vars": "error",
        "quotes": [2, "single", { "avoidEscape": true }]
    }
}